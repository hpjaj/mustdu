<!DOCTYPE html>
<html>
<head>
  <title>Mustdu</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  
  <% if flash[:notice] %>
    <div><%= flash[:notice] %></div>
  <% elsif flash[:error] %>
    <div><%= flash[:error] %></div>
  <% elsif flash[:alert] %>
    <div><%= flash[:alert] %></div>
  <% end %>
  
<%= yield %>

</body>
</html>
